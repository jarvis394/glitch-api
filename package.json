{
  "name": "glitch-api",
  "version": "3.1.0",
  "description": "Library for using Glitch API ⚙️",
  "main": "./lib/index.js",
  "scripts": {
    "start": "serve lib/docs",
    "test": "jest test",
    "build": "webpack -c",
    "build:docs": "typedoc --out ./lib/docs --mode file --name glitch-api --theme ./docs/theme ./src",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier src/**/{.[^.],}* --config .prettierrc --write"
  },
  "dependencies": {
    "algoliasearch": "^4.2.0",
    "node-fetch": "^2.6.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://glapi.ml",
  "repository": {
    "url": "https://github.com/jarvis394/glitch-api/"
  },
  "files": [
    "lib/index.js"
  ],
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "js",
    "api"
  ],
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.43",
    "@types/node-fetch": "^2.5.7",
    "@types/socket.io-client": "^1.4.33",
    "@types/ws": "^6.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "bufferutil": "^4.0.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "sass": "^1.26.7",
    "serve": "^11.3.1",
    "ts-jest": "^26.1.0",
    "typedoc": "^0.15.8",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
